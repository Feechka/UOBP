prefix="/usr"
exec_prefix="${prefix}"
datarootdir="${prefix}/share"
bindir="${exec_prefix}/bin"
sbindir="${exec_prefix}/sbin"
libexecdir="${exec_prefix}/libexec"
datadir="${datarootdir}"
sysconfdir="/etc"
sharedstatedir="${prefix}/com"
localstatedir="/var"
libdir="${exec_prefix}/lib"
includedir="${prefix}/include"
oldincludedir="/usr/include"
infodir="${datarootdir}/info"
mandir="/usr/share/man"

BRLTTY_VERSION="4.4"
BRLTTY_EXECUTE_ROOT=""
BRLTTY_PROGRAM_DIRECTORY="${exec_prefix}/bin"
BRLTTY_DRIVERS_DIRECTORY="${exec_prefix}/lib/brltty"
BRLTTY_TABLES_DIRECTORY="/etc/brltty"
BRLTTY_MANPAGE_DIRECTORY="/usr/share/man"
BRLTTY_INCLUDE_DIRECTORY="${prefix}/include/brltty"

BRLAPI_VERSION="0.5"
BRLAPI_RELEASE="0.5.7"
BRLAPI_AUTH="brlapi.key"
